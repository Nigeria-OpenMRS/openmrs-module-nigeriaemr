<htmlform>
	<!-- Autogenerated example form  (template from 01-Nov-2010 -->
	<macros>
		paperFormId = (Fill this in)
		headerColor =#009d8e
		fontOnHeaderColor = white
	</macros>

	<style>
		.section {
        border: 1px solid $headerColor;
        padding: 2px;
        text-align: left;
        margin-bottom: 1em;
        }
        .sectionHeader {
        background-color: $headerColor;
        color: $fontOnHeaderColor;
        display: block;
        padding: 2px;
        font-weight: bold;
        }
        .alnright { text-align: right; }
        td {
        border: none;
        }

        table th, table td {
        padding: 5px 10px;
        border: None;
        }
        table th, table td {
        padding: 5px 10px;
        /* border: 1px solid #DDD; */
        }
        table tr {
        border: None;
        }
	</style>

	<span style="float:right">Paper Form ID: $paperFormId</span>
	<section headerLabel=" Client Tracking and Termination">
		<table cellpadding="10" cellspacing="7" style="width: 80%; margin-left:10%;">
		<tr>
				<td>Date:</td>
				<td><encounterDate default="today"/></td>
			
				<td>Location:</td>
				<td><encounterLocation/></td>
			</tr>

			<tr>
			<td>Provider:</td>
				<td><encounterProvider/></td>
				<td>Serial Enrollment No.:</td>
				<td><obs conceptId="165560"  /></td>
			</tr>
            <tr>
				<td>Sex:</td>
				<td><obs conceptId="1533"  /></td>
			
				<td>Hospital (Unit) No.:</td>
				<td><obs conceptId="164812" class="hospitalno" /></td>
			</tr>
            <tr>
				<td>Reason for Tracking:</td>
				<td><obs conceptId="165460" /></td>
			
				<td>Patient Contact Address / Ph. Number:</td>
				<td><obs conceptId="162725" /></td>
			</tr>
            <tr>
				<td>Guardian / Treatment Partner's Name:</td>
				<td><obs conceptId="161135" /></td>
			
				<td>Date of Last Actual Contact/ Appointment:</td>
				<td><obs conceptId="164093" /></td>
			</tr>
                     
			<tr>
				<td>Date of Missed Scheduled Appointment:</td>
				<td colspan="2"></td>
			</tr>


		</table>
	</section>

<section headerLabel="Attempted Contact">
